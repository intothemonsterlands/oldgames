if %location% == goblinforestclearing2 goto goblinforestclearing2
if %location% == goblinforestclearing3 goto goblinforestclearing3
if %location% == goblinforestclearing_sidepath goto goblinforestclearing_sidepath
if %location% == goblinforestclearing4 goto goblinforestclearing4
)
if %escapechance% GTR %luck% (
echo  -----------------------------------
echo   You turn to run, but trip on a
echo   root protruding from the ground.
echo.
